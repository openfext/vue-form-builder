{
  "name": "element-form-builder",
  "version": 2,
  "alias": "element-form-builder",
  "builds": [{ "src": "**", "use": "@now/static" }],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/fonts/(.*)", "dest": "/fonts/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    {
      "src": "^/(favicon|manifest|precache|service-worker|robots)(.*)",
      "dest": "/$1$2"
    },
    { "src": ".*", "dest": "/index.html" }
  ]
}
